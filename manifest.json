{
  "name": "Vayo",
  "version": "1.0.4",
  "description": "Save your links like a pro",
  "manifest_version": 3,
  "author": "Gustavo Fior",
  "permissions": ["activeTab", "cookies"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Vayo",
    "default_icon": "chrome_icon.png"
  },
  "host_permissions": [
    "*://www.vayo.cc/"
  ],
  "icons": {
    "16": "chrome_icon.png",
    "32": "chrome_icon.png",
    "48": "chrome_icon.png",
    "128": "chrome_icon.png"
  }
}
